{
    "name": "AAS",
    "description": "Angular-auto-setup",
    "version":"0.1.0",
    "mainModule": "aasApp",
    "utilModule": "util",
    "tagname":"aas",
    "settings":{},
    "externanalAngularModule": [
        "ui.bootstrap","ngAnimate"
    ],
    "dependencies": {

    },
    "apiBaseurl":"",
    "api": {
        "users":"http://localhost:3000/posts/"
    },
    "apps": {
        "home": {
            "moduleName": "home",
            "paths": {
                "home": "module/home/js/Home"
            },
            "templateUrl": "module/home/template/Home.html"
        }
    } ,
    "assets": {
        "{{name}}": {
            "moduleName": "{{name}}",
            "paths": {
                "{{name}}": "assets/{{name}}/js/{{capitalize}}",
                "{{name}}Controllers": "assets/{{name}}/js/{{capitalize}}Controllers",
                "{{name}}Directives":"assets/{{name}}/js/{{capitalize}}Directives",
                "{{name}}Factories":"assets/{{name}}/js/{{capitalize}}Factories",
                "{{name}}Services":"assets/{{name}}/js/{{capitalize}}Services",
             "{{name}}Filters":"assets/{{name}}/js/{{capitalize}}Filters",
             "{{name}}Constants":"assets/{{name}}/js/{{capitalize}}Constants",
             "{{name}}Listeners":"assets/{{name}}/js/{{capitalize}}Listeners",
             "{{name}}Events":"assets/{{name}}/js/{{capitalize}}Events"
             },
            "templateUrl": "assets/{{name}}/template/{{capitalize}}.html"
        },


    },
    "routeAppMapping": {

    },
    "defaultRouting": "/",
    "routeAssetMapping": {

    },
    "unittest": {

    }
}