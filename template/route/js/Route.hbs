/**
 * Provides the base Widget {{capitalize}}
 *
 * @module {{name}}
 * @submodule {{pref}}App
 */
define(['LoadComponent'], function (LoadComponent) {
    'use strict';
    /**
     * Class to hmome module use define the collection of compononts in side the template
     *
     * @class {{capitalize}}
     * @constructor
     * @extends Clazz.LoadComponent
     * @module {{name}}
     */
    var {{capitalize}}=Clazz.extend(
    LoadComponent,{
        initialize:function(){
            this.templateUrl= Clazz.config.apps.{{name}}.templateUrl;
            /**
             * @restrict A
             *
             * @description
             * Using {{name}} module gets the collection of elements from template and loads the appropriate
             * components with the help of angular directives. This element is automatically loaded from
             * angular route.
             *
             *
             * ```html
             * <{{pref}}:{{name}}></{{pref}}:{{name}}>
             * ```
             *
             * @element {{pref}}{{capitalize}}
             */
            this.myContainer="{{pref}}{{capitalize}}";
            this.moduleName = Clazz.config.apps.{{name}}.moduleName;
            this.mymodule = angular.module(this.moduleName);
        }
    });
    return new {{capitalize}}();
});
